{
  "name": "javascript-collections",
  "version": "0.0.27",
  "description": "JavaScript implementation of different collections",
  "keywords": [
    "collection",
    "datastruct",
    "linkedlist",
    "queue",
    "stack",
    "hashset",
    "heap"
  ],
  "browser": "dist/javascript-collections.min.js",
  "main": "dist/javascript-collections.min.js",
  "license": "GPL-3.0",
  "scripts": {
    "test": "jest --coverage",
    "lint": "./node_modules/.bin/gulp lint",
    "compile": "./node_modules/.bin/gulp build",
    "build": "npm run lint && npm run compile && npm test"
  },
  "author": "Marcos Tischer Vallim <tischer@gmail.com>",
  "contributors": [
    "Rafael Malveis Ximenes <rmalveis@gmail.com>",
    "Leandro Nunes Fantinatto <dr1design@gmail.com>"
  ],
  "dependencies": {},
  "devDependencies": {
    "browserify": "^14.4.0",
    "coveralls": "^2.13.1",
    "eslint": "^4.1.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-eslint": "^4.0.0",
    "gulp-uglifyjs": "^0.6.2",
    "gulp-util": "^3.0.8",
    "jest": "^20.0.4",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/main/**/*.js"
    ],
    "coverageDirectory": "coverage/",
    "coverageReporters": [
      "lcov",
      "text-summary"
    ]
  }
}
